{
  "name": "rill-analytics-extension",
  "version": "0.1.0",
  "description": "Data analysis and report generation using Rill's metrics layer",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "link": "gemini extensions link .",
    "unlink": "gemini extensions uninstall rill-analytics-extension",
    "clean": "rm -rf dist",
    "lint": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1",
    "google-auth-library": "^10.4.0",
    "googleapis": "^161.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@eslint/js": "^9.24.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.30.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rilldata/rill",
    "directory": "extensions/gemini"
  },
  "author": {
    "name": "Rill Data",
    "email": "support@rilldata.com",
    "url": "https://rilldata.com"
  }
}
