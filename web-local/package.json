    "d3-format": "^3.1.0",
    "d3-scale-chromatic": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-time": "^3.0.0",
    "d3-time-format": "^4.1.0",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "glob": "^7.2.0",
    "immer": "^9.0.12",
    "isomorphic-unfetch": "^3.1.0",
    "module-alias": "^2.2.2",
    "pgsql-ast-parser": "^10.3.1",
    "socket.io": "^4.4.1",
